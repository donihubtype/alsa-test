{
  "scripts": {
    "deploy_production": "bash ../bin/deploy.sh true alsa ALSA_PRODUCTION",
    "quick_deploy_production": "bash ../bin/deploy.sh false alsa ALSA_PRODUCTION",
    "deploy_staging": "botonic deploy -c build_staging -b ALSA_STAGING",
    "build": "exit 1; Use `npm run deploy_staging` or `npm run deploy_production` instead",
    "build_webchat": "ENVIRONMENT=production webpack --env target=webchat --mode=production",
    "build_production": "ENVIRONMENT=production webpack --env target=all --mode=production",
    "build_staging": "ENVIRONMENT=staging webpack --env target=all --mode=production",
    "build_local": "ENVIRONMENT=local webpack --env target=all --mode=development",
    "lint": "npm run lint-core -- --fix && npm run prettier",
    "lint-ci": "npm run lint-core -- -c .eslintrc-ci.js && npm run prettier",
    "lint-core": "npm run lint-test && eslint_d --cache --quiet '{src,tests}/**/*.[t]{s,sx}'",
    "lint-test": "tsc --project tsconfig.tests.json ",
    "prettier": "prettier --no-error-on-unmatched-pattern --write '**/*.md' '**/*.css' '.*.js' '*.js{,on}'",
    "start": "ENVIRONMENT=local webpack-dev-server --env target=dev  --mode=development",
    "test": "jest  --passWithNoTests",
    "preversion": "npm run lint-ci",
    "# version issue": "See https://github.com/npm/cli/issues/2010. Temporarily checking all files have been git-added",
    "postversion": "git add package.json package-lock.json && git diff --exit-code && git commit && git push && git push --tags"
  },
  "name": "alsa",
  "version": "1.0.0",
  "dependencies": {
    "@botonic/core": "^0.30.0",
    "@botonic/plugin-flow-builder": "^0.30.0",
    "@botonic/plugin-hubtype-analytics": "^0.30.0",
    "@botonic/react": "^0.30.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/pro-regular-svg-icons": "^6.3.0",
    "@fortawesome/pro-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "1.6.2",
    "axios": "^1.6.8",
    "he": "^1.2.0",
    "react-redux": "^7.2.9",
    "styled-components": "^5.3.11",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@botonic/dx": "^0.30.0",
    "@hot-loader/react-dom": "^16.14.0",
    "@hubtype/flow-tester": "^0.6.0-alpha.1",
    "@types/react": "^16.14.56",
    "@types/react-dom": "^16.9.24",
    "@types/react-redux": "^7.1.33",
    "@types/react-stars": "^2.2.4",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "clean-webpack-plugin": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "csstype": "^2.6.21",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-react-parser": "^0.7.1",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "react-hot-loader": "4.12.21",
    "source-map-loader": "^0.2.4",
    "stream-browserify": "^3.0.0",
    "unorm": "^1.6.0"
  },
  "files": [
    "bin/**",
    "doc/**",
    "lib/**",
    "src/**",
    "README.md"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}